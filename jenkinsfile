pipeline{
        agent any
        stages{
            stage('Build'){
                steps{
                    mvn 
                }
            }
            
            stage('Test'){
                steps{
                    mvn test
                }
             }
            
            stage('Deploy'){
                steps{
                    sh java -jar /var/jenkins_home/workspace/springpenguin-training 
                }
            }
        }
}
